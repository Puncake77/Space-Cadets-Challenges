init X;
init Y = 10;
init Z = 7;
init boolX;
init boolY;
init boolZ;
init boolNot = 1;
if Y > X then;
	incr boolY;
else;
	clear boolY;
endif;
if X >= 2 then;
	clear boolX;
else;
	incr boolX;
endif;
if Z == 7 then;
	incr boolZ;
else;
	clear boolZ;
endif;
if X <> 0 then;
	clear boolNot;
endif;
end;